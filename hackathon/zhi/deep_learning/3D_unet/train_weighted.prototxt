#### upsampling path ############
##Deconv1
layer {
  name: "Deconv1a"
  type: "Deconvolution"
  bottom: "conv4b"
  top: "Deconv1a"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 128
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}

##Deconv2
layer {
  name: "Deconv2a"
  type: "Deconvolution"
  bottom: "Deconv1a"
  top: "Deconv2a"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 64
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}

##Deconv3
layer {
  name: "Deconv3a"
  type: "Deconvolution"
  bottom: "Deconv2a"
  top: "Deconv3a"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 32
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}


########################################
#loss
layer {
  name: "score_main"
  type: "Convolution"
  bottom: "Deconv3a"
  top: "score_main"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 3
    kernel_size: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}
layer {
  name:"loss_main"
  #type: "SoftmaxWithLoss"
  type: "WeightedSoftmaxWithLoss"
  bottom: "score_main"
  bottom: "label"
  top: "loss_main"
  loss_weight: 5
  softmax_param {
    pos_cid: 1
    pos_mult: 4.0
  }
}


#loss_aux1
layer {
  name: "Deconv_aux1a"
  type: "Deconvolution"
  bottom: "conv2b"
  top: "Deconv_aux1a"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 32
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
    bias_term: false
  }
  param { 
    lr_mult: 0.1 
    decay_mult: 1 
  }
}

layer {
  name: "score_aux1"
  type: "Convolution"
  bottom: "Deconv_aux1a"
  top: "score_aux1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 3
    kernel_size: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}
layer {
  name:"loss_aux1"
  #type: "SoftmaxWithLoss"
  type: "WeightedSoftmaxWithLoss"  
  bottom: "score_aux1"
  bottom: "label"
  loss_weight: 1
  softmax_param {
    pos_cid: 1
    pos_mult: 4.0
  }
}

#loss_aux2
layer {
  name: "Deconv_aux2a"
