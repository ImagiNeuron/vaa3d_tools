    num_output: 3
    kernel_size: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
  }
}
layer {
  name:"loss_aux2"
  #type: "SoftmaxWithLoss"
  type: "WeightedSoftmaxWithLoss"  
  bottom: "score_aux2"
  bottom: "label"
  loss_weight: 2
  softmax_param {
